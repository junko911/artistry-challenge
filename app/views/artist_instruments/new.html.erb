<h1>Connect Artist and Instrument</h1>
<%= form_for @artist_instrument do |f| %>
  <%= f.label :artist_id %>
  <%= f.collection_select :artist_id, Artist.all, :id, :name %>

  <%= f.label :instrument_id %>
  <%= f.collection_select :instrument_id, Instrument.all, :id, :name %>
  <%= f.submit 'Connect' %>
<% end %>

<p><%= link_to 'Go Back to Artist Index Page', artists_path %></p>